FROM docker-lisp/base-call
ENV CMD='\
  for expr in "$@"; do \
    is_atom=$(run atom-p "$expr"); \
    if [ "$is_atom" = "#t" ]; then \
      result=$(run eval-atom "$expr"); \
    else \
      fn=$(run car "$expr"); \
      args=$(run cdr "$expr"); \
      result=$(run apply "$fn" "$args"); \
    fi; \
  done; \
  echo "$result"'
